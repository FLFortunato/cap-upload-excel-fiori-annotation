{
  "name": "booksui",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "cds-plugin-ui5": "^0.9.3",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "hybrid": "cds watch --profile hybrid",
    "watch-booksui": "cds watch --open booksui/index.html?sap-ui-xx-viewCache=false --livereload false",
    "undeploy": "cf undeploy booksui --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "private": true,
  "cds": {
    "requires": {
      "[production]": {
        "auth": "xsuaa"
      },
      "destinations": true,
      "html5-repo": true
    }
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/booksui"
  ]
}